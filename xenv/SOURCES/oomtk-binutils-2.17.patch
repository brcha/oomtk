diff -Naur binutils-2.17/bfd/config.bfd binutils-2.17-new/bfd/config.bfd
--- binutils-2.17/bfd/config.bfd	2006-04-05 14:41:57.000000000 +0200
+++ binutils-2.17-new/bfd/config.bfd	2006-11-24 02:48:13.000000000 +0100
@@ -452,6 +452,28 @@
     targ_defvec=bfd_elf32_i386_vec
     targ_selvecs=i386coff_vec
     ;;
+  arm-*-oomtk*)
+    targ_defvec=bfd_elf32_bigarm_vec
+    targ_selvecs=bfd_elf32_littlearm_vec
+    ;;
+  i[3-7]86-*-oomtk*)
+    targ_defvec=bfd_elf32_i386_vec
+    ;;
+  x86_64-*-oomtk*)
+    targ_defvec=bfd_elf64_x86_64_vec
+    targ_selvecs="bfd_elf32_i386_vec"
+    ;;
+  powerpc-*-oomtk*)
+    targ_defvec=bfd_elf32_powerpc_vec
+    targ_selvecs="rs6000coff_vec"
+    targ_cflags=-DSMALL_ARCHIVE
+    ;;
+#ifdef BFD64
+  powerpc64-*-oomtk*)
+    targ_defvec=bfd_elf64_powerpc_vec
+    targ_selvecs="bfd_elf64_powerpcle_vec bfd_elf32_powerpc_vec bfd_elf32_powerpcle_vec rs6000coff_vec rs6000coff64_vec"
+    ;;
+#endif
   i[3-7]86-*-solaris2*)
     targ_defvec=bfd_elf32_i386_vec
     targ_selvecs=i386coff_vec
